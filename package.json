{
    "name": "@ballware/meta-backend",
    "version": "6.0.0-alpha1",
    "description": "Ballware Application Builder - Client for metadata provider backend",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
    },
    "files": [
        "dist/**/*"
    ],
    "exports": {
        ".": "./dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/frankball/ballware-meta-backend.git"
    },
    "keywords": [],
    "author": "frank@ballmeyer.com",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/frankball/ballware-meta-backend/issues"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^4.10.0",
        "@typescript-eslint/parser": "^4.10.0",
        "eslint": "^7.16.0",
        "eslint-config-prettier": "^7.1.0",
        "prettier": "^2.2.1",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "json5": "^2.1.3",
        "axios": "^0.21.0",
        "@ballware/meta-interface": "^6.0.0-alpha5"
    },
    "homepage": "https://github.com/frankball/ballware-meta-backend#readme",
    "prettier": {
        "printWidth": 120,
        "trailingComma": "all",
        "singleQuote": true
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "plugins": [
            "@typescript-eslint"
        ],
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended",
            "eslint-config-prettier"
        ],
        "ignorePatterns": [
            "node_modules",
            "dist",
            "spec"
        ]
    }
}
