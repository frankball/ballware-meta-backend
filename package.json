{
  "name": "@ballware/meta-backend",
  "version": "6.0.0-beta1",
  "description": "Ballware Application Builder - Client for metadata provider backend",
  "license": "MIT",
  "author": "frank@ballmeyer.com",
  "homepage": "https://github.com/frankball/ballware-meta-backend#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frankball/ballware-meta-backend.git"
  },
  "bugs": {
    "url": "https://github.com/frankball/ballware-meta-backend/issues"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "json5": ">=2.1.3",
    "axios": ">=0.21.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/ballware-meta-backend.esm.js",
  "size-limit": [
    {
      "path": "dist/ballware-meta-backend.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ballware-meta-backend.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@ballware/meta-interface": "^6.0.0-beta1"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "husky": "^4.3.7",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3",
    "json5": "^2.1.3",
    "axios": "^0.21.0"
  }
}
